{
    "name": "laxove-php/dotenv",
    "type": "library",
    "keywords": [
        "php-library",
        "php-component",
        "php7.2",
        "loader",
        "config"
    ],
    "description": "DotEnv component for LaxovePHP.",
    "homepage": "https://github.com/LaxovePHP/DotEnv",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/Nenglish7",
            "role": "Developer"
        },
        {
            "name": "LaxovePHP Contributors",
            "homepage": "https://github.com/orgs/LaxovePHP/people",
            "role": "Contribute"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "LaxovePHP\\DotEnv\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

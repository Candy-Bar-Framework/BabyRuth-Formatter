{
    "name": "laxove-php/dotenv",
    "type": "library",
    "keywords": [
        "php-library",
        "php-component",
        "php7-2",
        "json-formatter",
        "yaml-formatter",
        "php72",
        // "php73",
        // "php74"
    ],
    "description": "An extremely fast XML, JSON, and YAML formatter.",
    "homepage": "https://github.com/genial-framework/BabyRuth-Formatter",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/obozaxy",
            "role": "Develop/Maintain"
        },
        {
            "name": "Repository Contributors",
            "homepage": "https://github.com/genial-framework/BabyRuth-Formatter/graphs/contributors",
            "role": "Contribute"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaxovePHP\\DotEnv\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

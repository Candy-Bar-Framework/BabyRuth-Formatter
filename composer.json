{
    "name": "genial-framework/babyruth-formatter",
    "type": "library",
    "keywords": [
        "php-library",
        "php-component",
        "genial-framework",
        "xml-formatter",
        "json-formatter",
        "yaml-formatter",
        "php72",
        "php73",
        "php74"
    ],
    "description": "An extremely fast XML, JSON, and YAML formatter.",
    "homepage": "https://github.com/genial-framework/BabyRuth-Formatter",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/obozaxy",
            "role": "Develop/Maintain"
        },
        {
            "name": "Repository Contributors",
            "homepage": "https://github.com/genial-framework/BabyRuth-Formatter/graphs/contributors",
            "role": "Contribute"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Genial\\BabyRuth\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "ext-xdebug": "*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
